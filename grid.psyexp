        <Param val="trial_order = [];&amp;#10;training_order = [];&amp;#10;for (var trial, _pj_c = 0, _pj_a = util.range(num_of_trials), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    trial = _pj_a[_pj_c];&amp;#10;    if ((trial &gt;= (num_of_trials / 2))) {&amp;#10;        trial_order.push(1);&amp;#10;    } else {&amp;#10;        trial_order.push(0);&amp;#10;    }&amp;#10;}&amp;#10;for (var trial, _pj_c = 0, _pj_a = util.range(num_of_training), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    trial = _pj_a[_pj_c];&amp;#10;    if ((trial &gt;= (num_of_training / 2))) {&amp;#10;        training_order.push(1);&amp;#10;    } else {&amp;#10;        training_order.push(0);&amp;#10;    }&amp;#10;}&amp;#10;random.shuffle(trial_order);&amp;#10;random.shuffle(training_order);&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# Create trials&amp;#10;trial_order = []&amp;#10;training_order = []&amp;#10;&amp;#10;# Regular trials&amp;#10;for trial in range(num_of_trials):&amp;#10;    if trial &gt;= num_of_trials/2:&amp;#10;        trial_order.append(1)&amp;#10;    else:&amp;#10;        trial_order.append(0)&amp;#10;&amp;#10;# Training trials&amp;#10;for trial in range(num_of_training):&amp;#10;    if trial &gt;= num_of_training/2:&amp;#10;        training_order.append(1)&amp;#10;    else:&amp;#10;        training_order.append(0)&amp;#10;&amp;#10;# Shuffle the trials order&amp;#10;random.shuffle(trial_order)&amp;#10;random.shuffle(training_order)" valType="extendedCode" updates="constant" name="Begin Routine"/>
