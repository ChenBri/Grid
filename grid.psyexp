        <Param val="# Define the number of training trials&amp;#10;num_of_training = 4&amp;#10;&amp;#10;# Define the number of the experiment trials&amp;#10;num_of_trials = 4" valType="extendedCode" updates="constant" name="Before Experiment"/>
